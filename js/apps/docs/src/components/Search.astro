---
const PUBLIC_KEY = "pk_RtRSUN4XMvMSiznJKHFMJFwh";
const ENDPOINT = "https://cloud.getcanary.dev";
---

<canary-styles-starlight>
  <canary-provider-cloud key={PUBLIC_KEY} endpoint={ENDPOINT}>
    <canary-modal>
      <canary-trigger slot="trigger"></canary-trigger>
      <canary-content slot="content">
        <canary-input-search slot="input-search"></canary-input-search>
        <canary-input-ask slot="input-ask"></canary-input-ask>
        <canary-result-search slot="result-search"></canary-result-search>
        <canary-result-ask slot="result-ask"></canary-result-ask>
        <canary-callout-discord
          slot="callout"
          url="https://discord.gg/Y8bJkzuQZU"></canary-callout-discord>
      </canary-content>
    </canary-modal>
  </canary-provider-cloud>
</canary-styles-starlight>
